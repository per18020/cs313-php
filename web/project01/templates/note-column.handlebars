<div>
    <div class="box ">
        <div class="level">
            <div class="level-left">
                <div class="level-item">
                    <p>Write a new note</p>
                </div>
            </div>
            <div class="level-right">
                <div class="level-item">
                    
                </div>
            </div>
        </div>
    </div>
    {{#each notes}}
    <div class="box note-column-box" note_id="{{this.note.id}}">
        <div class="level">
            <div class="level-left">
                <div class="level-item">
                    <h1>{{this.note.title}}</h1>
                </div>
            </div>
            <div class="level-left">
                <div class="level-item">
                    <p>{{this.note.creation}}</p>
                </div>
            </div>
        </div>
        <div>
            Preview text goes here.
        </div>
    </div>
    {{/each}}
</div>